@model Person
@{
    ViewData["Title"] = "EditPerson";
}

<li class="main-li" id="@("person" + Model.Id)">
    <form method="post">
        @Model.Id |
        <input id="createName" type="text" name="Name" value="@Model.Name">
        <input id="createPhoneNumber" type="number" name="PhoneNumber" value="@Model.PhoneNumber" />
        <input id="createCity" type="text" name="City" value="@Model.City" />

        <button class="btn btn-success aUpdate" onclick="LinkEdit('@Url.Action("EditComplete", "Home", new { id = Model.Id } )' , '@("person" + Model.Id)', '@Model.Id')">Update</button>
    </form>
</li>